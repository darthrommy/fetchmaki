{
  "name": "typedfetch",
  "version": "0.3.0",
  "description": "A simple fetch wrapper for Typescript",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./zod": {
      "types": "./dist/types/zod.d.ts",
      "import": "./dist/zod.js",
      "require": "./dist/cjs/zod.js"
    },
    "./supabase": {
      "types": "./dist/types/supabase.d.ts",
      "import": "./dist/supabase.js",
      "require": "./dist/cjs/supabase.js"
    }
  },
  "typesVersions": {
    "*": {
      "zod": [
        "./dist/types/zod.d.ts"
      ],
      "supabase": [
        "./dist/types/supabase.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "concurrently -m 8 'npm:build:*' && rimraf dist/types/index.js && npm run copy",
    "build:base": "rollup -c",
    "build:zod": "rollup -c --config-zod",
    "build:supabase": "rollup -c --config-supabase",
    "prepublishOnly": "npm run build",
    "copy": "shx cp ./package.cjs.json ./dist/cjs/package.json && shx cp ./package.cjs.json ./dist/types/package.json",
    "lint": "eslint src/**",
    "pack": "npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darthrommy/typedfetch.git"
  },
  "author": "darthrommy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darthrommy/typedfetch/issues"
  },
  "homepage": "https://github.com/darthrommy/typedfetch#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.8.1",
    "shx": "^0.3.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.16"
  },
  "dependencies": {
    "zod": "^3.20.0"
  },
  "peerDependencies": {
    "zod": "^3.20.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
