{
  "name": "@darthrommy/fetches",
  "version": "0.2.3",
  "description": "A simple fetch wrapper for Typescript",
  "main": "./index.js",
  "types": "./index.d.ts",
  "typesVersions": {
    "<4.5": {
      "esm/*": [
        "*"
      ],
      "*": [
        "*"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.mjs"
      },
      "types": "./index.d.ts",
      "module": "./esm/index.js",
      "default": "./index.js"
    },
    "./*": {
      "import": {
        "types": "./esm/*.d.ts",
        "default": "./esm/*.mjs"
      },
      "types": "./*.d.ts",
      "module": "./esm/*.js",
      "default": "./*.js"
    }
  },
  "files": [
    "**"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "concurrently -m 8 'npm:build:*'",
    "build:base": "rollup -c",
    "build:zod": "rollup -c --config-zod",
    "build:supabase-edge": "rollup -c --config-supabase-edge",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**",
    "copy": "shx cp -r dist/* . && shx rm -rf .github dist .eslint* .gitignore .prettier* package-lock.json rollup.config.mjs",
    "test:build": "npm run copy && npm pack --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darthrommy/fetches.git"
  },
  "author": "darthrommy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darthrommy/fetches/issues"
  },
  "homepage": "https://github.com/darthrommy/fetches#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.8.1",
    "shx": "^0.3.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.16"
  },
  "dependencies": {
    "zod": "^3.20.0"
  },
  "peerDependencies": {
    "zod": "^3.20.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
