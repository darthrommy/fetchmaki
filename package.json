{
  "name": "@darthrommy/fetches",
  "version": "0.2.4",
  "description": "A simple fetch wrapper for Typescript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "module": "./dist/esm/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.mjs"
      },
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "module": "./dist/esm/*.js",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "concurrently -m 8 'npm:build:*'",
    "build:base": "rollup -c",
    "build:zod": "rollup -c --config-zod",
    "build:supabase-edge": "rollup -c --config-supabase-edge",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**",
    "pack": "npm pack --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darthrommy/fetches.git"
  },
  "author": "darthrommy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darthrommy/fetches/issues"
  },
  "homepage": "https://github.com/darthrommy/fetches#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.8.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.16"
  },
  "dependencies": {
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "zod": "^3.20.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
